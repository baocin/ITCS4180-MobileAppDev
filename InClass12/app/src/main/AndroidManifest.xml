<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.baocin.inclass12">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.github.baocin.inclass12.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.github.baocin.inclass12.ExpenseList" />
        <activity android:name="com.github.baocin.inclass12.AddExpense" />
        <activity android:name="com.github.baocin.inclass12.CreateAccount" />
        <activity android:name="com.github.baocin.inclass12.Details"></activity>
    </application>

</manifest>
